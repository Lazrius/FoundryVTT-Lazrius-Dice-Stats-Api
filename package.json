{
	"name": "lazrius-dice-stats-backend",
	"version": "1.0.0",
	"description": "Backend for FoundryVTT Dice Stats Module",
	"main": "server.js",
	"scripts": {
		"dev": "nodemon",
		"build": "npx tsc",
		"run": "node dist/Server.js",
		"faker": "ts-node -- Source/Faker.ts",
		"migration:generate": "typeorm-ts-node-commonjs migration:generate -d Source/Connection.ts",
		"migration:up": "typeorm-ts-node-commonjs migration:run -d Source/Connection.ts",
		"migration:down": "typeorm-ts-node-commonjs migration:revert -d Source/Connection.ts"
	},
	"author": "Lazrius",
	"license": "GPL-3.0",
	"dependencies": {
		"class-validator": "^0.13.2",
		"express": "^4.16.2",
		"http-errors": "^1.7.1",
		"http-status-codes": "^2.2.0",
		"moment": "^2.29.4",
		"mssql": "^7.3.5",
		"mysql2": "^2.3.3",
		"pg": "^8.7.3",
		"typeorm": "^0.3.7",
		"yargs": "^17.3.1"
	},
	"devDependencies": {
		"@faker-js/faker": "^7.3.0",
		"@types/express": "4.17.13",
		"@types/http-errors": "1.8.2",
		"@types/node": "^14.18.12",
		"@types/validator": "^13.7.4",
		"@types/yargs": "^17.0.8",
		"@typescript-eslint/eslint-plugin": "4.33.0",
		"@typescript-eslint/parser": "4.33.0",
		"chalk": "^4.1.2",
		"class-transformer": "^0.5.1",
		"class-transformer-validator": "^0.9.1",
		"eslint": "7.32.0",
		"eslint-config-prettier": "8.4.0",
		"eslint-plugin-import": "2.25.4",
		"nodemon": "2.0.15",
		"prettier": "2.5.1",
		"ts-node": "^10.8.1",
		"typescript": "4.5.5"
	}
}
